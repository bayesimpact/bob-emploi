{
  "compilerOptions": {
    "baseUrl": ".",
    "esModuleInterop": true,
    "isolatedModules": true,
    "lib": ["es6", "es2020"],
    "module": "esNext",
    "moduleResolution": "node",
    "noEmit": true,
    "noUnusedLocals": true,
    "resolveJsonModule": true,
    "strict": true,
    "target": "esNext"
  }
}
