  "scripts": {
    "clean": "rimraf dist/*",
    "copy": "copyfiles -u 1 ./src/index.html ./src/favicon.ico ./dist",
    "dist": "npm run clean && npm run copy && REACT_WEBPACK_ENV=dist webpack --bail",
    "lint:js": "eslint . --ext js,jsx",
    "lint:configs": "pjv && jsonlint -c -q .eslintrc.json",
    "lint:text": "! ( grep â€™ -r ./src && echo ERROR: Replace curly quotes by straight quotes )",
