{
  "compilerOptions": {
    "baseUrl": ".",
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react",
    "lib": ["dom", "dom.iterable", "es5", "es6"],
    "module": "esNext",
    "moduleResolution": "node",
    "noEmit": true,
    "noUnusedLocals": true,
    "paths": {
      "api/*": [
        "bob_emploi/frontend/api/*"
      ],
      "components/*": [
        "src/components/*"
      ],
      "images/*": [
        "src/images/*"
      ],
      "store/*": [
        "src/store/*"
      ],
      "translations/*": [
        "src/translations/*"
      ]
    },
    "resolveJsonModule": true,
    "strict": true,
    "target": "esNext"
  },
  "include": [
    "bob_emploi/frontend/api/*.d.ts",
    "custom.d.ts",
    "src",
    "test"
  ],
  "exclude": [
    "bob_emploi/analytics/*"
  ]
}
