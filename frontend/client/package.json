    "check-types": "tsc",
    "clean": "rimraf dist/*",
    "copy": "copyfiles ./favicon.ico ./dist",
    "dist": "npm run clean && npm run copy && REACT_WEBPACK_ENV=dist CLIENT_VERSION=$CLIENT_VERSION webpack --bail",
    "download": "node download.js",
    "i18n": "run-p i18n:*",
    "i18n:extract": "babel --config-file ./i18n.babelrc 'src/**/*.{js,jsx,ts,tsx}' > /dev/null",
